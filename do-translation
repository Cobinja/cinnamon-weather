msgfmt -c -v po/pt.po -o locale/pt/LC_MESSAGES/cinnamon-weather@mockturtl.gmo
msgfmt -c -v po/ro.po -o locale/ro/LC_MESSAGES/cinnamon-weather@mockturtl.gmo
